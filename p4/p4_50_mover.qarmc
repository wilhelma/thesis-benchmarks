% (Nmover)
s1_0_0(PC,PCp,XW,XR) :- 
    step1(X,MX,PC,Xp,MXp,PCp,XW,XR)
    ,(XW>=1;XR>=1),lh1(PC,MXH), MXH=<0.
s1_1_0(PC,PCp,XW,XR) :- 
    step1(X,MX,PC,Xp,MXp,PCp,XW,XR)
    ,(XW>=1;XR>=1),lh1(PC,MXH), MXH>=1.
