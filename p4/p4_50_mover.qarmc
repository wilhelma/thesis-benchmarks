                                   ;s1_1_0(_,_,XWj,XRj)
                                   ;s1_0_1(_,_,XWj,XRj)
                                   ;s1_1_1(_,_,XWj,XRj)
                                   ;s3_0_0(_,_,XWj,XRj)
