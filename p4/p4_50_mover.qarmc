                                   ;s2_1_1(_,_,XWj,XRj)))
     ;(s3_1_0(PC,PCp,XW,XR), (s1_0_0(_,_,XWj,XRj)
                                   ;s1_0_1(_,_,XWj,XRj)
                                   ;s2_0_0(_,_,XWj,XRj)
