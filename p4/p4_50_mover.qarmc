     ;(s3_1_1(PC,PCp,XW,XR), (s1_0_0(_,_,XWj,XRj)
                                   ;s2_0_0(_,_,XWj,XRj)))
    ), (XW>=1,XWj>=1
       ;XW>=1,XRj>=1
       ;XR>=1,XWj>=1).

